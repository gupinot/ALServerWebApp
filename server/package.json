  "scripts": {
    "build": "npm run clean && mkdir -p build && cp -rf ../dist build/web && cp *.js *.json build && cp -rf lib scripts build/ && npm run package",
    "clean": "rm -rf build $npm_package_name-$npm_package_version $npm_package_name-$npm_package_version.tgz *.log",
    "package": "ln -s build $npm_package_name-$npm_package_version && tar zcvfL $npm_package_name-$npm_package_version.tgz $npm_package_name-$npm_package_version/",
    "start": "npm run build && node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
